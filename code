#include<iostream>
#include<cmath>
#include<cstdlib>
#include<cstring>
using namespace std;
int main()
{
    double a,b,c,x1,x2;
    char x;
    cout<<"a=";
    cin>>a;
    cout<<"b=";
    cin>>b;
    cout<<"enter the value of c=";
    cin>>c;

    x1= (-b+sqrt((pow(b,2))-4*a*c))/2*a ;
    x2= (-b-sqrt((pow(b,2))-4*a*c))/2*a ;

    cout << "Your X's are "<<x1<< " and "<<x2;

    return 0;
}
